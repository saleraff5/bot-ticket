# Token do bot Discord (obtenha em https://discord.com/developers/applications)
DISCORD_TOKEN=seu_token_aqui

# ID do servidor Discord
GUILD_ID=id_do_seu_servidor

# ID do canal onde ficará o embed principal
TICKET_CHANNEL_ID=id_do_canal_tickets

# ID do canal de logs (opcional)
LOG_CHANNEL_ID=id_do_canal_logs

# IDs dos cargos que podem ver os tickets
STAFF_ROLE_IDS=id_cargo1,id_cargo2,id_cargo3

# Categoria onde os tickets serão criados (opcional)
TICKET_CATEGORY_ID=id_da_categoria

# URLs das imagens para o embed principal (opcional)
EMBED_IMAGE_URL=https://exemplo.com/imagem_principal.png
EMBED_THUMBNAIL_URL=https://exemplo.com/logo_pequeno.png

# Credenciais do Cloudinary para transcrições (obtenha em https://cloudinary.com/console)
CLOUDINARY_CLOUD_NAME=seu_cloud_name
CLOUDINARY_API_KEY=sua_api_key
CLOUDINARY_API_SECRET=sua_api_secret

